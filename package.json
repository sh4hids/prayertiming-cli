{
  "name": "prayertiming-cli",
  "version": "0.0.0-development",
  "description": "A small cli tool to calculate muslim prayer times based on coordinates and timezone",
  "main": "dist/index.js",
  "engines": {
    "node": "14.16.0"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "nodemon dev.js",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "test": "jest src",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "prayertiming": "./dist/index.js",
    "pt": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sh4hids/prayertiming-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sh4hids/prayertiming-cli/issues"
  },
  "homepage": "https://github.com/sh4hids/prayertiming-cli#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.2"
  },
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "cli-table": "^0.3.6",
    "cliui": "^6.0.0",
    "configstore": "^5.0.1",
    "date-fns": "^2.21.1",
    "figlet": "^1.5.0",
    "inquirer": "^8.0.0",
    "minimist": "^1.2.5",
    "prayertiming": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/git",
      "@semantic-release/npm",
      {
        "assets": [
          "dist",
          "README.md",
          "package.json"
        ],
        "message": ":bookmark: Release ${nextRelease.version} [skip ci]"
      }
    ]
  }
}
